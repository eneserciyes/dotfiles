Host cims
    HostName access.cims.nyu.edu
    Port 22
    Compression yes
    User me2646
    IdentityFile ~/.ssh/id_ed25519
    AddKeysToAgent yes
    ServerAliveInterval 60

Host linserv
    User me2646
    HostName linserv1.cims.nyu.edu
    ProxyJump cims

Host greene
    HostName log-2.hpc.nyu.edu
    Port 22
    Compression yes
    User me2646
    IdentityFile ~/.ssh/id_ed25519
    ForwardAgent yes
    ServerAliveInterval 60

Host greene-compute 
    HostName cm014
    User me2646
    ProxyJump greene
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    LogLevel ERROR

Host nuc
    HostName 10.19.248.70
    User robot-lab
    LogLevel ERROR
    DynamicForward 1337

